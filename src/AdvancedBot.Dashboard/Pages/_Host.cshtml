@page "/"
@namespace AdvancedBot.Dashboard.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
  <meta charset="utf-8">
  <title>GLR Discord Bot</title>
  <link rel="preconnect" href="https://fonts.gstatic.com">
  <link href="favicon.ico" type="image/x-icon" rel="shortcut icon">
  <link href="_content/MatBlazor/dist/matBlazor.css" rel="stylesheet" />
  <link href="css\app.css" type="text/css" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900" rel="stylesheet" crossorigin="anonymous">
  <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900" rel="stylesheet" crossorigin="anonymous">
  <link href="https://fonts.googleapis.com/css2?family=Material+Icons" rel="stylesheet" crossorigin="anonymous">
  <script src="_content/MatBlazor/dist/matBlazor.js"></script>
  <meta name="description" content="Dashboard panel for Galaxy Life Reborn Discord moderators.">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
</head>
<body class="mdc-typography">
    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" />
    </app>

    <div id="blazor-error-ui">
        <div class="mdc-snackbar mdc-snackbar--leading">
            <div class="mdc-snackbar__surface" role="status" aria-relevant="additions">
                <div class="mdc-snackbar__label" aria-atomic="false">
                    An unhandled exception has occurred. See browser dev tools for details.
                </div>
                <div class="mdc-snackbar__actions" aria-atomic="true">
                    <button type="button" class="mdc-button mdc-snackbar__action">
                        <div class="mdc-button__ripple"></div>
                        <a class="mdc-button__label" onclick="window.location.reload();">Reload</a>
                    </button>
                </div>
            </div>
        </div>
    </div>
    <script src="_framework/blazor.server.js" type="text/javascript"></script>
</body>
</html>
